

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id              String     @id @default(uuid())
  userId          String     
  duration        Int
  effort          Int
  createdAt       DateTime   @default(now())
}
